{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "build" /* Base directory to resolve non-absolute module names. */,
    "baseUrl": "./",
    "paths": {
      "*": ["*", "src/*"]
    },
    "typeRoots": ["@types", "../../node_modules/@types"]
  },
  "include": ["./src"]
}
